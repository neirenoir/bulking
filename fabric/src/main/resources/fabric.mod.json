{
  "schemaVersion": 1,
  "id": "bulking",
  "version": "${version}",
  "name": "Bulking",
  "description": "A plugin for Diet inspired by SOL: Valheim. Do away with hunger: just stuff your stomach for awesome buffs!",
  "authors": [
    "neirenoir"
  ],
  "contact": {
    "homepage": "https://github.com/neirenoir/bulking",
    "sources": "https://github.com/neirenoir/bulking"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.neire.mc.bulking.fabric.BulkingFabric"
    ],
    "client": [
      "dev.neire.mc.bulking.fabric.client.BulkingFabricClient"
    ]
  },
  "mixins": [
    "bulking.mixins.json",
    "bulking-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.10",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.11.0",
    "forgeconfigapiport": ">=8.0.2",
    "diet": "2.1.1"
  },
  "recommends": {
    "forgeconfigscreens": ">=8.0.2"
  }
}
